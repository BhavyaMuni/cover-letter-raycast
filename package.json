{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "cover-letter-raycast",
  "title": "Cover Letter Generator",
  "description": "Generates cover letter and downloads it to folder specified.",
  "icon": "command-icon.png",
  "author": "bhavya_muni",
  "owner": "bhavyamuni",
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Generate Cover Letter",
      "description": "Template for submitting a form",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.61.0",
    "dotenv": "^16.3.1",
    "googleapis": "^128.0.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "18.18.4",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "npx ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
